{
  "name": "data-harvester",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "main": "src/app.ts",
  "scripts": {
    "ci": "npm ci 2>&1 | grep -v 'deprecated' | grep -v 'EBADENGINE' || npm ci",
    "start": "NODE_PATH=./node_modules NODE_OPTIONS='--no-deprecation' tsx src/app.ts",
    "build": "tsc",
    "test": "NODE_ENV=test jest --config jest.config.js",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "lint": "eslint --ext .ts,.tsx ./src",
    "check-format": "prettier --check 'src/**/*.{js,jsx,ts,tsx}'",
    "migrate": "node-pg-migrate",
    "migrate:create": "npm run migrate -- create --migration-file-language sql",
    "generate:schemas": "ts-to-zod src/models/commonStructure.ts src/models/commonStructure.zod.gen.ts"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.ics.muni.cz:514266/data-harvester.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@eslint/js": "^9.21.0",
    "@reduxjs/toolkit": "^2.5.1",
    "@rtk-query/codegen-openapi": "^2.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.13.1",
    "@types/node-fetch": "^2.6.13",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "babel-jest": "^29.7.0",
    "dotenv-expand": "^12.0.3",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.1.0",
    "jest": "^29.7.0",
    "jsdom": "^26.1.0",
    "node-pg-migrate": "^8.0.3",
    "openapi-typescript": "^7.6.1",
    "pg": "^8.16.3",
    "pg-mem": "^3.0.5",
    "prettier": "^3.5.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "bottleneck": "^2.19.5",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "flyway": "^0.1.9",
    "lodash": "^4.17.21",
    "node-fetch": "^2.7.0",
    "semantic-ui-css": "^2.5.0",
    "swagger-jsdoc": "^7.0.0-rc.6",
    "swagger-ui-express": "^5.0.1",
    "ts-to-zod": "^4.0.0",
    "typescript-eslint": "^8.32.1"
  }
